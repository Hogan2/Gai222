<UserControl x:Class="GMap.NET.WindowsPresentation.Text_Marker"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid>
        <TextBox x:Name="myText" Text="新建文本" Foreground="Lime" Height="30" FontSize="22" Background="Transparent" 
                 BorderBrush="Transparent" BorderThickness="1" Focusable="{Binding IsTextFocusable}"/>
    </Grid>
</UserControl>
